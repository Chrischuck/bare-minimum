{
  "name": "grade-calculator",
  "version": "1.0.0",
  "description": "clean/modern website to calculate your final grade (with weights)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline",
    "build": "NODE_ENV=production webpack -p --config ./webpack.production.config.js",
    "start": "node server.js",
    "postinstall": "NODE_ENV=production&& webpack -p --config webpack.production.config.js"
  },
  "keywords": [
    "grade",
    "calculator",
    "final",
    "grade",
    "school",
    "education",
    "gpa"
  ],
  "author": "Chris Chuck",
  "license": "MIT",
  "dependencies": {
    "preact": "^7.1.0",
    "preact-compat": "^3.10.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.0",
    "sweetalert2": "^6.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-closure-elimination": "^1.1.14",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.1.0",
    "install": "^0.8.4",
    "nodemon": "^1.11.0",
    "npm": "^4.0.5",
    "offline-plugin": "^4.5.5",
    "purifycss-webpack-plugin": "^2.0.3",
    "react-hot-loader": "^1.3.1",
    "static-site-generator-webpack-plugin": "^3.1.0",
    "style-loader": "^0.13.1",
    "svg-loader": "0.0.2",
    "url-loader": "^0.5.7",
    "web-app-manifest-loader": "^0.1.1",
    "webpack": "^1.14.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ],
    "plugins": [
      "closure-elimination",
      "transform-react-constant-elements",
      "transform-react-remove-prop-types"
    ]
  },
  "eslint": {
    "extends": "airbnb",
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/jsx-curly-spacing": [
        2,
        "always"
      ],
      "react/prefer-stateless-function": [
        0
      ],
      "arrow-body-style": [
        "error",
        "never"
      ]
    }
  }
}
